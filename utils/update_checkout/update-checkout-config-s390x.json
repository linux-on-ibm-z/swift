                "llvm": "stable",
                "clang": "stable",
                "swift": "master",
                "lldb": "stable",
                "cmark": "master",
                "llbuild": "master",
                "swiftpm": "master",
                "swift-syntax": "master",
                "swift-stress-tester": "master",
                "compiler-rt": "stable",
                "swift-corelibs-xctest": "master",
                "swift-corelibs-foundation": "master",
                "swift-corelibs-libdispatch": "master",
                "swift-integration-tests": "master",
                "swift-xcode-playground-support": "master",
                "ninja": "release",
                "icu": "release-61-1"
            }
        },
        "swift-5.0-s390x-04-01-19" : {
            "aliases": ["swift-5.0-s390x-04-01-19"],
            "repos": {
                "llvm": "swift-5.0-s390x-04-01-19",
