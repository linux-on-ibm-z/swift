{
    "ssh-clone-pattern": "git@github.com:%s.git",
    "https-clone-pattern": "https://github.com/%s.git",
    "repos" : {
        "llvm": {
            "remote": { "id": "linux-on-ibm-z/swift-llvm" } },
        "clang": {
            "remote": { "id": "linux-on-ibm-z/swift-clang" } },
        "swift": {
            "remote": { "id": "linux-on-ibm-z/swift" } },
        "lldb": {
            "remote": { "id": "linux-on-ibm-z/swift-lldb" } },
        "cmark": {
            "remote": { "id": "linux-on-ibm-z/swift-cmark" } },
        "llbuild": {
            "remote": { "id": "linux-on-ibm-z/swift-llbuild" } },
        "swiftpm": {
            "remote": { "id": "linux-on-ibm-z/swift-package-manager" } },
        "swift-syntax": {
            "remote": { "id": "linux-on-ibm-z/swift-syntax" } },
        "swift-stress-tester": {
            "remote": { "id": "linux-on-ibm-z/swift-stress-tester" } },
